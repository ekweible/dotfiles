#!/bin/sh

#########################################
# Utility Functions


# Switch primary Dart install to Dart 1.x
function d1() {
  brew unlink dart
  brew unlink dart@1
  brew switch dart@1 1.24.3
  brew link --force dart@1
  dart --version
}

# Switch primary Dart install to Dart 2.x
function d2() {
  brew unlink dart
  brew unlink dart@1
  brew switch dart 2.0.0
  brew link dart
  dart --version
}

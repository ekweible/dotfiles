#!/bin/sh

#########################################
# Utility Functions


# Switch primary Dart install to Dart 1.x
function d1() {
  brew unlink dart
  brew unlink dart@1
  brew switch dart@1 1.24.3
  brew link --force dart@1
  dart --version
}

# Switch primary Dart install to latest Dart 2.x (2.2.0)
function d2() {
  brew unlink dart
  brew unlink dart@1
  brew switch dart 2.2.0
  brew link dart
  dart --version
}

# Switch primary Dart install to Dart 2.1.1
function d211() {
  brew unlink dart
  brew unlink dart@1
  brew switch dart 2.1.1
  brew link dart
  dart --version
}

# Switch primary Dart install to Dart 2.1.0
function d210() {
  brew unlink dart
  brew unlink dart@1
  brew switch dart 2.1.0
  brew link dart
  dart --version
}

# Switch primary Dart install to Dart 2.0.0
function d200() {
  brew unlink dart
  brew unlink dart@1
  brew switch dart 2.0.0
  brew link dart
  dart --version
}

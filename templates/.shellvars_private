#!/usr/bin/env bash

export DOTFILES_PROFILE="DOTFILES_PROFILE"
export DOTFILES_PRIVATE="$HOME/dev/dotfiles_private/$DOTFILES_PROFILE"
export HOMEDIR_PRIVATE="$DOTFILES_PRIVATE/homedir"
PRIVATE_SHELLVARS="$HOMEDIR_PRIVATE/.shellvars" \
    && test -f $PRIVATE_SHELLVARS \
    && source $PRIVATE_SHELLVARS
